#include<bits/stdc++.h>
#include<string.h>
using namespace std;
int main()
{
    int a,x,b,i,j,count=0;
    string ra;
    cin>> x;
    getchar();
    getline(cin,ra);
    int l = ra.length();
    for(i=0; i<l; i++)
    {
        if(ra[i]!=' '){
            ra[i]-=x;
        if(ra[i]<'a')
            ra[i]='z'+ra[i]-'a'+1;}
    }
    cout<< ra;
    return 0;
}
